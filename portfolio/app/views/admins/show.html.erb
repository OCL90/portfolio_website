<div class="col-lg-12 text-right" id="logout">
    <button class="btn btn-danger"><a href="/logout">Logout</a></button>
</div>
<div class="col-lg-8 col-lg-offset-2">
  <table class="table table-bordered">
    <thead>
      <tr>
        <th colspan="4" class="text-center">Potential Clients</th>
      </tr>
      <tr>
        <th>Name</th>
        <th>Email</th>
        <th>Subject</th>
        <th>Message</th>
      </tr>
    </thead>
    <% @client.each do |c| %>
    <tbody>
      <tr>
        <td><%=c.name%></td>
        <td><%=c.email%></td>
        <td><%=c.subject%></td>
        <td><div><%=c.message%></div></td>
      </tr>
    </tbody>
    <% end %>
  </table>
</div>