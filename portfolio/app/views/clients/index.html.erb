<div class="skills"></div>
<div class="projects"></div>
<div class="col-lg-12" id="contact_me">
  <div class="row">
    <div class="col-lg-7">
      <div class="row">
        <div class="col-lg-12 text-center contact_title">
          <h2><%= image_tag("black_email.png", :alt => "black_email") %> GET IN TOUCH WITH ME</h2>
          <h4>Need a website? A web application? I am available for freelance work, I want to hear about your projects.</h4>
        </div>
      </div>
      <div class="row">
        <div class="col-lg-12" id="form">
          <form class="col-lg-10 col-lg-offset-4" action="/clients" method="post">
          <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
            <div class="form-group row">
              <label for="example-text-input" class="col-lg-1 col-xs-3 col-form-label">Name:</label>
              <div class="col-lg-5 col-xs-9">
                <input type="text" name="name" class="form-control" id="formGroupExampleInput" placeholder="Name">
              </div>
            </div>
            <div class="form-group row">
              <label for="example-text-input" class="col-lg-1 col-xs-3 col-form-label">Email:</label>
              <div class="col-lg-5 col-xs-9">
                <input type="email" name="email" class="form-control" id="formGroupExampleInput" placeholder="Email">
              </div>
            </div>
            <div class="form-group row">
              <label for="example-text-input" class="col-lg-1 col-xs-3 col-form-label">Subject:</label>
              <div class="col-lg-5 col-xs-9">
                <input type="text" name="subject" class="form-control" id="formGroupExampleInput" placeholder="Subject">
              </div>
            </div>
            <div class="form-group row">
              <label for="example-text-input" class="col-lg-1 col-xs-3 col-form-label">Message:</label>
              <div class="col-lg-5 col-xs-9">
                <textarea class="form-control" name="message" id="exampleTextarea" rows="3" placeholder="Message..."></textarea>
              </div>
            </div>
            <div class="form-group row">
              <div class="col-xs-12 col-lg-6 text-right">
                <button type="submit" class="btn btn-primary btn-lg">Submit</button>
              </div>
            </div>
              <% if flash[:success]%>
                <p class="success"><%= flash[:success] %><p>
              <% end %>
              <% if flash[:errors]%>
                <% flash[:errors].each do |e|%>
                  <p class="errors"><%= e %><p>
                <% end %>
              <% end %>
          </form>
        </div>
      </div>
    </div>
    <div class="col-lg-4 google_map">
      <div id="irving_map"></div>
    </div>
  </div>
</div>
<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDcH9F1fBaHQZFarAjp2zjreVTy9V-3WOg" type="text/javascript"></script>
<!-- Irving_Map -->
<script>
var myCenter1 = new google.maps.LatLng(32.9359, -96.9585);

function initialize() {
var mapProp1 = {
center:myCenter1,
zoom:12,
scrollwheel:false,
draggable:false,
mapTypeId:google.maps.MapTypeId.ROADMAP
};

var map1 = new google.maps.Map(document.getElementById("irving_map"),mapProp1);

var marker1 = new google.maps.Marker({
position:myCenter1,
});

marker1.setMap(map1);
}

google.maps.event.addDomListener(window, 'load', initialize);
</script>
